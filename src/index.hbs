    </head>
    <body>
        {{#if staticProps}}
            <script>
                window.__INITIAL_DATA__ = <%- JSON.stringify({{staticProps}}) %>;
            </script>
        {{else}}
            <script>
                window.__INITIAL_DATA__ = {};
            </script>
        {{/if}}
        <div id="root">{{#if body}} {{{body}}} {{/if}}</div>
        {{#if scriptTags}}
            {{{scriptTags}}}
        {{else}}
            <script src="/bundles/bundle.js"></script>
        {{/if}}
    </body>
</html>