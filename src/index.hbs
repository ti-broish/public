        <meta name="theme-color" content="#000000" />
        {{#if additionalStyleTags}} {{{additionalStyleTags}}} {{/if}}
        {{#if linkTags}} {{{linkTags}}} {{/if}}
        {{#if styleTags}} {{{styleTags}}} {{/if}} 
